{
    "title": "Patient",
    "description": "Patient infomation",
    "comment": "Schema of patient information from redcap.",
    "id": "/profiles/patient.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "identifyingProperties": [
      "uuid",
      "accession"
    ],
    "required": [
      "accession"
    ],
    "additionalProperties": false,
    "mixinProperties": [
      {
        "$ref": "mixins.json#/schema_version"
      },
      {
        "$ref": "mixins.json#/accession"
      },
      {
        "$ref": "mixins.json#/accessioned_status"
      },
      {
        "$ref": "mixins.json#/form_status"
      },
      {
        "$ref": "mixins.json#/submitted"
      },
      {
        "$ref": "mixins.json#/uuid"
      }
    ],
    "type": "object",
    "properties": {
      "schema_version": {
        "default": "1"
      },
      "accession": {
        "title": "ADC Subject ID",
        "description": "ADC Subject ID,Patient ID, Accession",
        "comment": "CE personnel are allowed to set this value.",
        "type": "string",
        "permission": "import_items",
        "pattern": "^([0-9]{6})$",
        "uniqueKey": true
      },
      "last": {
        "title": "Patient Last Name",
        "type": "string"
      },
      "first": {
        "title": "Patient First Name",
        "comment": "Required field",
        "type": "string"
      },
      "mi": {
        "title": "Patient Middle Initial",
        "type": "string"
      },
      "adcid_mstrid": {
        "title": "ADC ID",
        "description": "Patient's initials + DOB (eg. FMLmmddyyyy)",
        "type": "string"
      },
      "examiner": {
        "title": "Examiner's initials",
        "comment": "Required field",
        "type": "string"
      },
      "patiddate": {
        "title": "Date of Exam/Contact",
        "comment": "Required field",
        "type": "string"
      },
      "category": {
        "title": "1. Patient Initial Category",
        "comment": "Required field",
        "type": "string",
        "enum": [
          "1 ADC Core",
          "2 Consult",
          "3 Research Only",
          "4 Control",
          "5 ADC relative",
          "6 autopsy only",
          "7 Autopsied demented relative of ADC patient",
          "8 Autopsied non-demented relative of ADC patient",
          "9 Autopsied/non-clinic/control",
          "99 Unknown/not done"
        ]
      },
      "site": {
        "title": "2. Responsible Site",
        "comment": "Required field",
        "type": "string",
        "enum": [
          "1 UTSW",
          "2 Native American Satellite",
          "3 Other"
        ]
      },
      "gender": {
        "title": "3. Gender",
        "comment": "Required field",
        "type": "string",
        "enum": [
          "1 Male",
          "0 Female"
        ]
      },
      "dob": {
        "title": "4. Date of birth  mm/dd/yyyy",
        "description": "Patient's date of birth.Date can be submitted as MM/DD/YYYY.",
        "comment": "Required field",
        "type": "string",
        "anyOf": [
          {
            "format": "date"
          }
        ]
      },
      "ssnmed": {
        "title": "5. Social Security Number",
        "comment": "Required field",
        "type": "string"
      },
      "astonid": {
        "title": "AstonID (MRN)",
        "description": "UTSouthwestern Aston hospital Medical Number",
        "comment": "@HIDDEN,CE personnel are allowed to set this value.",
        "type": "string",
        "permission": "import_items",
        "uniqueKey": true
      },
      "parklandid": {
        "title": "ParklandID (MRN)",
        "description": "UTSouthwestern Parkland hospital medical Number",
        "comment": "@HIDDEN,CE personnel are allowed to set this value.",
        "type": "string",
        "permission": "import_items",
        "uniqueKey": true
      },
      "hand": {
        "title": "Handedness",
        "comment": "@HIDDEN",
        "type": "string",
        "enum": [
          "1 Right",
          "2 Left",
          "3 Ambidextrous"
        ]
      },
      "educat": {
        "title": "6. Years of education  Enter '99' if Unknown number",
        "type": "string"
      },
      "excmenret": {
        "title": "7. If less than 6 years of education does the subject have a work history sufficient to exclude mental retardation?",
        "type": "string",
        "enum": [
          "1 Yes",
          "0 No",
          "9 Unknown"
        ]
      },
      "occupat": {
        "title": "8. Occupation (during most of adult life)",
        "type": "string"
      },
      "preflang": {
        "title": "9. Language to be used for testing",
        "type": "string",
        "enum": [
          "1 English",
          "2 Spanish",
          "9 Other/Unknown(only one may be checked and used throughout the study)"
        ]
      },
      "spanish": {
        "title": "10. Ethnicity (self-reported) Is subject Spanish, Hispanic or Latino",
        "type": "string",
        "enum": [
          "1 Yes",
          "0 No",
          "9 Unknown"
        ]
      },
      "racial": {
        "title": "11. Racial Category",
        "type": "string",
        "enum": [
          "1 Alaskan Native",
          "2 Black or African American",
          "3 White",
          "4 American Indian",
          "5 Asian",
          "6 Native Hawaiian or other Pacific Islander",
          "7 Multiple races",
          "8 Other",
          "9 Unknown or not reported"
        ]
      },
      "raceother": {
        "title": "Other, please specify",
        "type": "string"
      },
      "aitribe": {
        "title": "12. If American Indian checked, a) what tribe?",
        "type": "string",
        "enum": [
          "1 Cherokee",
          "2 Choctaw",
          "3 Creek",
          "4 Chickasaw",
          "5 Other"
        ]
      },
      "tribedesc": {
        "title": "Other, please specify",
        "type": "string"
      },
      "tribeperc": {
        "title": "b) % Native American",
        "type": "string"
      },
