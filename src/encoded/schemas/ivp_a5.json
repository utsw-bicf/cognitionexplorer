                    "enum": [
                        "0 No",
                        "1 Yes",
                        "9 Unknown"
                    ]
                },
                "hattyear": {
                    "title": "2a2. Year of most recent heart attack",
                    "description": "(1900 - current year, 9999 = Unknown)",
                    "type": "string",
                    "pattern": "^[1][9][0-9][0-9]|[2][0][0-9][0-9]|[9999]$"
                },
                "cvafib": {
                    "title": "2b. Atrial fibrillation",
                    "type": "string",
                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
                    ]
                },
                "cvangio": {
                    "title": "2c. Angioplasty / endarterectomy / stent",
                    "type": "string",
                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
