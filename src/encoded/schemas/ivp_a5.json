                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
                    ]
                },
                "cvpacdef": {
                    "title": "2e. Pacemaker and/or defibrillator",
                    "type": "string",
                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
                    ]
                },
                "cvchf": {
                    "title": "2f. Congestive heart failure",
                    "type": "string",
                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
                    ]
                },
                "cvangina": {
                    "title": "2g. Angina",
                    "type": "string",
                    "enum": [
                        "0 Absent",
                        "1 Recent/Active",
                        "2 Remote/Inactive",
                        "9 Unknown"
                    ]
