                },
                "hvalve": {
                    "title": "21. Procedure: heart valve replacement or repair",
                    "type": "string",
                    "enum": [
                        "0 No",
                        "1 Yes",
                        "8 Not assessed"
                    ]
                },
                "antienc": {
                    "title": "22. Antibody-mediated encephalopathy",
                    "type": "string",
                    "enum": [
                        "0 No",
                        "1 Yes",
                        "8 Not assessed"
                    ]
