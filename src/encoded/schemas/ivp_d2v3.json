                            "enum": [
                                "0",
                                "1 Spine"
                            ]
                        },
                        "artunkn": {
                            "title": "11b4.  Arthritis region affected",
                            "type": "string",
                            "enum": [
                                "0",
                                "1 Unknown"
                            ]
                        }
                    }
                   
                },
                "urineinc": {
                    "title": "12. Incontinence - urinary",
                    "type": "string",
                    "enum": [
                        "0 No",
                        "1 Yes",
                        "8 Not assessed"
                    ]
                },
                "bowlinc": {
                    "title": "13. Incontinence - bowel",
                    "type": "string",
                    "enum": [
                        "0 No",
                        "1 Yes",
                        "8 Not assessed"
                    ]
                },
                "sleepap": {
                    "title": "14. Sleep apnea",
                    "type": "string",
                    "enum": [
                        "0 No",
                        "1 Yes",
                        "8 Not assessed"
                    ]
                },
