        "verb_fluency_phone_test": {
            "title": "12. Verbal Fluency: Phonemic Test",
            "type": "object",
            "properties": {
                "udsverfc": {
                    "title": "12a. Number of correct F-words generated in 1 minute",
                    "description": "(If test not completed, enter reason code, 95-98) and SKIP TO QUESTION 12d.) (0-40, 95-98)",
