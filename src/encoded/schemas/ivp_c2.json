                "craftdti": {
                    "title": "9c. Delay time (minutes)  (99=Unknown)  ",
                    "description": "(0-85 minutes 99=Unknown)",
                    "type": "string",
                    "pattern":"^[1-7]?[0-9]$|^[8][0-5]$|^99$"

