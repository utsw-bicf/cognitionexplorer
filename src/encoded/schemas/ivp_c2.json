                "mintpcng": {
                    "title": "11e. Phonemic cues: Number given",
                    "description": "(0-32)",
                    "type": "string",
                    "pattern":"^[1-2]?[0-9]$|^[3][0-2]$"

                },
                "mintpcnc": {
                    "title": "11f. Phonemic cues: Number correct with cue  (88=not applicable)  ",
                    "description": "(0-32, 88)",
                    "type": "string",
                    "pattern":"^[1-2]?[0-9]$|^[3][0-2]$|^88$"

                }
            }
